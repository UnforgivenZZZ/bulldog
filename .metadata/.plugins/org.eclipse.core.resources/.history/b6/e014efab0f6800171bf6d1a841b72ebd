<%@ page import="java.util.*, com.products.*" %>
<!DOCTYPE html>
<html>

<head>
	<title>Student Tracker App</title>	
</head>

<%
	// get the students from the request object (sent by servlet)
	List<product> theProducts = 
					(List<product>) request.getAttribute("PRODUCT_LIST");
%>

<body>

	<div id="wrapper">
		<div id="header">
			<h2>FooBar University</h2>
		</div>
	</div>

	<div id="container">
	
		<div id="content">
		
			<table>
			
				<tr>
					<th>First Name</th>
					<th>Last Name</th>
					<th>Email</th>
				</tr>
				
				<% for (product tempStudent : theProducts) { %>
				
					<tr>
						<td> <%= tempStudent.getId() %> </td>
						<td> <%= tempStudent.getSku() %> </td>
						<td> <%= tempStudent.getPicurl() %> </td>
					</tr>
				
				<% } %>
				
			</table>
		
		</div>
	
	</div>
</body>


</html>








